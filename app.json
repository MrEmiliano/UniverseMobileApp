{
  "expo": {
    "name": "UniverseMobileApp",
    "slug": "UniverseMobileApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/load.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/load.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/load.png",
        "backgroundColor": "#121212"
      }
    },
    "web": {
      "favicon": "./app/assets/load.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
